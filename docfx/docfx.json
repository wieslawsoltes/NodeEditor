{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/NodeEditorAvalonia/NodeEditorAvalonia.csproj",
            "src/NodeEditorAvalonia.Model/NodeEditorAvalonia.Model.csproj",
            "src/NodeEditorAvalonia.Mvvm/NodeEditorAvalonia.Mvvm.csproj",
            "src/NodeEditorLogic.Core/NodeEditorLogic.Core.csproj",
            "src/NodeEditorLogic.Editor/NodeEditorLogic.Editor.csproj"
          ],
          "src": ".."
        }
      ],
      "properties": {
        "TargetFramework": "net10.0"
      },
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md",
          "apidoc/**.overwrite"
        ]
      }
    ],
    "dest": "../_site",
    "globalMetadata": {
      "_appTitle": "NodeEditor for Avalonia",
      "_appLogoPath": "images/logo.svg"
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
